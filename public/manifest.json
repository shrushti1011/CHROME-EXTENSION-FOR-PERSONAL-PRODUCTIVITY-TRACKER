
{
  "manifest_version": 3,
  "name": "FocusFlow - Productivity Dashboard",
  "version": "1.0.0",
  "description": "A customizable productivity dashboard for your new tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  }
}
